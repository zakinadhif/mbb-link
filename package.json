{
	"name": "mbb-link",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"packageManager": "pnpm@10.14.0",
	"scripts": {
		"cf-typegen": "pnpm exec wrangler types",
		"dev-host": "pnpm exec react-router dev --host",
		"dev": "pnpm exec react-router dev",
		"build": "pnpm exec react-router build",
		"deploy": "pnpm run build && pnpm exec wrangler deploy",
		"preview": "pnpm run build && pnpm exec vite preview",
		"db-studio:local": "cross-env TARGET=local pnpm exec drizzle-kit studio",
		"db-studio:remote": "cross-env TARGET=production pnpm exec drizzle-kit studio",
		"db-migrate:local": "cross-env TARGET=local pnpm exec drizzle-kit migrate",
		"db-migrate:remote": "cross-env TARGET=production pnpm exec drizzle-kit migrate",
		"db-generate-migration": "cross-env TARGET=local pnpm exec drizzle-kit generate",
		"typecheck": "pnpm run cf-typegen && pnpm exec react-router typegen && pnpm exec tsc -b"
	},
	"dependencies": {
		"@hookform/resolvers": "^5.0.1",
		"@icons-pack/react-simple-icons": "^13.5.0",
		"@radix-ui/react-checkbox": "^1.3.3",
		"@radix-ui/react-label": "^2.1.8",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-separator": "^1.1.8",
		"@radix-ui/react-slot": "^1.2.4",
		"@radix-ui/react-switch": "^1.2.6",
		"@react-router/fs-routes": "^7.5.3",
		"@tanstack/react-form": "^1.26.0",
		"@uidotdev/usehooks": "^2.4.1",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"dotenv": "^16.5.0",
		"drizzle-orm": "^0.43.1",
		"isbot": "^5.1.27",
		"lucide-react": "^0.554.0",
		"mimetext": "^3.0.27",
		"nanoid": "^5.1.5",
		"radix-ui": "^1.4.2",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-icons": "^5.5.0",
		"react-router": "^7.8.2",
		"remix-auth": "^4.2.0",
		"remix-auth-oauth2": "^3.4.0",
		"tailwind-merge": "^3.2.0",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.12.3",
		"@react-router/dev": "^7.8.2",
		"@tailwindcss/vite": "^4.1.4",
		"@types/node": "^20",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.2",
		"better-sqlite3": "^12.4.6",
		"cross-env": "^7.0.3",
		"drizzle-kit": "^0.31.1",
		"tailwindcss": "^4.1.4",
		"tsx": "^4.19.4",
		"tw-animate-css": "^1.2.9",
		"typescript": "^5.8.3",
		"vite": "^7.1.11",
		"vite-tsconfig-paths": "^5.1.4",
		"wrangler": "^4.34.0"
	}
}
